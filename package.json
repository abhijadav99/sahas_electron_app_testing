{
  "name": "electron_sahas",
  "version": "1.3.0",
  "description": "This is Windows app For Sahas",
  "main": "app.js",
  "scripts": {
    "start": "npm install && electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm install && electron-builder build --win --publish always"
    },
  "repository": {
    "type": "git",
    "url": "https://github.com/abhijadav99/sahas_electron_app_testing.git"
  },
  "keywords": [
    "Sahas"
  ],
  "build": {    
    "productName": "Sahas",
    "appId":"com.AJ.sahas",
    "directories": {
      "output": "builds/"
    },
    "publish": {
      "provider": "github",
      "releaseType": "draft"
    },
    "win": {
      "icon": "./assets/img/sahas.png",
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  
  "author": "Abhishek Jadav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abhijadav99/sahas_electron_app_testing/issues"
    },
  "homepage": "https://github.com/abhijadav99/sahas_electron_app_testing#readme",
  "devDependencies": {
    "electron-builder": "^22.4.1",
    "electron": "^27.1.0"
  }
}
